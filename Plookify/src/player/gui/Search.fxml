<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="player.logic.SearchController">
   <children>
      <Label fx:id="result" layoutX="8.0" layoutY="8.0" prefHeight="24.0" prefWidth="174.0">
         <font>
            <Font name="Lucida Sans Regular" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="searchResult" layoutX="1.0" layoutY="32.0" prefHeight="471.0" prefWidth="506.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="172.0" text="Name" />
          <TableColumn fx:id="lengthColumn" prefWidth="84.0" text="Length" />
            <TableColumn fx:id="artistColumn" prefWidth="154.0" text="Artist" />
            <TableColumn fx:id="genreColumn" prefWidth="93.0" text="Genre" />
        </columns>
         <contextMenu>
            <ContextMenu fx:id="options">
              <items>
                  <MenuItem fx:id="option1" mnemonicParsing="false" onAction="#addSong" text="Add song to Now Playing list" />
                <MenuItem fx:id="options2" mnemonicParsing="false" onAction="#addAll" text="Add all songs to Now Playing list" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
   </children>
</AnchorPane>
